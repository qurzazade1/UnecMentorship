 // 7)Sort a list of strings alphabetically:

            List<string> mylist1 = new List<string>()
            { "Ayan", "Burak", "Lala", "Robin", "Gigi", "Zayn"};
            mylist1.Sort();
            foreach (var word in mylist1)
            {
                Console.WriteLine(word);
            }

            //8) Determine if a list of numbers contains a specific value:
            List<int> mylist2 = new List<int>()
            { 1,3,6,7,9,1,0};
            var resp= mylist2.Contains(6);
            Console.WriteLine(resp);

            //9)Count the number of strings in a list that have a specific length:
            List<string> myarray8 = new List<string>()
            {"ayan","lala","leila","Mahiyya","Gulchohra","lalazar" , "Seizaar"};
            var respi = myarray8.Where(b => b.Length == 7).Count();
            Console.WriteLine(respi);
            Console.ReadLine(); 
