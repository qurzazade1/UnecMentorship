 int[] MYarray = { 1,3,4,4,4,5,6};
            int count = 1;
           
            for (int i=0; i<MYarray.Length-1; i++)
            {
                if (MYarray[i] == MYarray[i+1] && MYarray[i]==3)
                {  
                    count+=1;                
                }
                
            }
            Console.WriteLine(count);
